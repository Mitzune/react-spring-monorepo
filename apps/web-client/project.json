{
	"name": "web-client",
	"targets": {
		"lint": {
			"executor": "nx:run-commands",
			"options": {
				"command": "pnpm lint-staged",
				"cwd": "apps/web-client"
			}
		},
		"dev": {
			"executor": "nx:run-commands",
			"options": {
				"command": "pnpm dev",
				"cwd": "apps/web-client"
			}
		},
		"staging": {
			"executor": "nx:run-commands",
			"options": {
				"command": "pnpm dev --mode staging",
				"cwd": "apps/web-client"
			}
		},
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"command": "pnpm build",
				"cwd": "apps/web-client"
			}
		}
	}
}
